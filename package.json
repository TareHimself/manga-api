{
  "name": "manga-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "debug": "node ./dist/index.js --debug --no-cluster",
    "docker:run": "docker run -p 8888:8888 --name manga-api tarehimself/manga-api",
    "docker:build": "docker build -f Dockerfile . -t tarehimself/manga-api",
    "docker:br": "npm run docker:build && npm run docker:run",
    "build": "npm run clean && npx tsc",
    "watch": "npm run build && tsc-watch",
    "pretty": "npx prettier --write **/*.ts",
    "clean": "node ./scripts/clean.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.29.1",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "node-html-parser": "^6.1.13",
    "npm": "^10.8.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
